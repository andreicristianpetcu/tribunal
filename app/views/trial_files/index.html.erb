<h1>Listing trial_files</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trial_files.each do |trial_file| %>
      <tr>
        <td><%= link_to 'Show', trial_file %></td>
        <td><%= link_to 'Edit', edit_trial_file_path(trial_file) %></td>
        <td><%= link_to 'Destroy', trial_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trial file', new_trial_file_path %>
