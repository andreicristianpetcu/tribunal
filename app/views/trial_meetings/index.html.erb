<h1>Listing trial_meetings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trial_meetings.each do |trial_meeting| %>
      <tr>
        <td><%= link_to 'Show', trial_meeting %></td>
        <td><%= link_to 'Edit', edit_trial_meeting_path(trial_meeting) %></td>
        <td><%= link_to 'Destroy', trial_meeting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trial meeting', new_trial_meeting_path %>
