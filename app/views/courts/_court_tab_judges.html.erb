<div class="tab-pane fade in" id="judges">
<div class="bs-component">
  <p>judecători</p>
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <td>
          Număr dosar
        </td>
        <td>
          Categorie caz
        </td>
        <td>
          Stadiu Procesual
        </td>
      </tr>
    </thead>

    <tbody>
      <% @proceedings.each do |proceeding| %>
        <tr>
          <td>
            <%= proceeding.number %>
          </td>
          <td>
            <%= proceeding.case_type %>
          </td>
          <td>
            <%= proceeding.trial_status %>
          </td>
        </tr>
      <% end %>

    </tbody>
  </table>
  <%= paginate @proceedings.all, :param_name => :court_proceedings_page %>
</div>

</div>
